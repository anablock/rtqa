    <fields>
        <fullName>BW_Heat_Index__c</fullName>
        <externalId>false</externalId>
        <formula>if(BW_Indicator_Count__c &gt; 0,((BW_Rating_Total__c - BW_Minimum_Rating__c)/(BW_Maximum_Rating__c - BW_Minimum_Rating__c))*100,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Heat Index</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
